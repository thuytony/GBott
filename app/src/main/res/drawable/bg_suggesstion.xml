<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="@color/colorLine"/>
            <corners
                android:radius="250dp" />
            <padding
                android:left="5dp"
                android:top="0dp"
                android:right="5dp"
                android:bottom="0dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorBorderSuggesstion" />
        </shape>
    </item>

    <item android:state_focused="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="@color/colorLine"/>
            <corners
                android:radius="250dp" />
            <padding
                android:left="5dp"
                android:top="0dp"
                android:right="5dp"
                android:bottom="0dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorBorderSuggesstion" />
        </shape>
    </item>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="@color/colorBgFriendChat"/>
            <corners
                android:radius="250dp" />
            <padding
                android:left="5dp"
                android:top="0dp"
                android:right="5dp"
                android:bottom="0dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorBorderSuggesstion" />
        </shape>
    </item>
</selector>